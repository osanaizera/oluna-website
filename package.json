{
  "name": "oluna-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:watch": "jest --watch",
    "test:debug": "jest --watch --verbose",
    "analyze": "ANALYZE=true next build",
    "clean": "rm -rf .next out dist node_modules/.cache",
    "validate": "npm run format:check && npm run lint && npm run typecheck",
    "prepare-commit": "npm run validate",
    "lighthouse": "lhci autorun",
    "storybook": "echo 'Storybook not configured yet'",
    "check-updates": "npx npm-check-updates",
    "security-check": "npm audit --audit-level moderate",
    "bundle-analyze": "cross-env ANALYZE=true next build",
    "preinstall": "npx only-allow npm"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.19.1",
    "gtag": "^1.0.1",
    "js-cookie": "^3.0.5",
    "next": "^14.2.30",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "resend": "^4.6.0",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@types/jest": "29.5.14",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "22.10.2",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "8.18.1",
    "@typescript-eslint/parser": "8.18.1",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.18",
    "eslint-config-prettier": "9.1.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "prettier": "3.4.2",
    "tailwindcss": "^4.0.0",
    "typescript": "5.7.2"
  }
}
